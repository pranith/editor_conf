syntax on
filetype plugin on
set tabstop=4
set shiftwidth=4
" set expandtab
set tw=80

set hlsearch
set number
set autoindent
set cindent
set smartindent
set ignorecase

set nocp

" map <C-F12> :!ctags -R --c++-kinds=+p --fields=+iaS --extra=+q . /usr/include/*.h 

